<apex:page standardController="Apttus_Revenue2__RevRec_Period__c" 
	  	   extensions="Apttus_Revenue2.RollbackActualRevenueBatchController" 
           showHeader="true" sidebar="true" 
           action="{!init}" >	
           
	<apex:sectionHeader title="Rollback Revenue (Actual)"></apex:sectionHeader>
	
	<apex:form >
		<!--  required fields -->
		<apex:outputText value="{!Apttus_Revenue2__RevRec_Period__c.Apttus_Revenue2__Current_Period_Date__c}" rendered="false"/>
		
		<apex:pageBlock >
			<apex:pageMessages />
			<apex:outputPanel >
				<apex:outputText style="font-weight: bold" value="{!statusText}" />
			</apex:outputPanel>
		</apex:pageBlock>
	</apex:form>

</apex:page>