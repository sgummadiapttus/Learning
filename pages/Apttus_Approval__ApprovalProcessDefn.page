                <apex:panelGrid columns="1" rendered="{!steps.size == 0}">
                    <apex:outputText value="{!$Label.Apttus_Approval__NoRecordsToDisplay}" />
                </apex:panelGrid>

            </apex:pageBlockSection>
        </apex:pageBlock>

        <!-- Final Approval Actions -->
        <apex:pageBlock id="idFinalApprovalActionsBlock"
